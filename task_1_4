int main () {
    int a = 9; //works
    bool b = false; //works
    char c = 'a'; // works
    double d = 1.3; //works
    int const five = 5; //works 'five' cannot be changed
    double e = a/two; //'two' is not defined

    five = d; //can't change const variable, can't assign double to int without loss of information

    return 0; //end main
}